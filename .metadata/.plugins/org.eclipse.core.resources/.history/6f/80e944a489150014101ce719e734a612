package MockPackage;

import java.util.ArrayList;

public class Words {
	
	public static void main(String [] args) {
		
		ArrayList<Integer> commonWords = new ArrayList<Integer>();
		
		for (int i = 0; i < args.length; i++) {
			if (args[i].length() > 100) {
				continue;
			}
			commonWords.add(args[i]);
		}
		
	}
	
}