package lab02;

public class HelloWorld {
	public static void main(String[] args) {
		System.out.println("Hello, World!");
	
		int [][] grid = new int [5][10];
		for (int i = 0; i < 10; i++) {
			for (int j = 0; j < 5; j++) {
				System.out.print(grid[i][j]);
			}
			System.out.println();
		}
		
	
		   
		}
}

