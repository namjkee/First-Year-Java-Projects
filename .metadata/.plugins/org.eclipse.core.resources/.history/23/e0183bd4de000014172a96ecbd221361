package lab05;

import static org.junit.Assert.*;

import org.junit.Test;

public class StatsTest {
	int[] testData = new int[]{1,2,3};

	@Test
	public void testAverage() {
		int[] testData = new int[]{1,2,3,4,5};
		assertEquals(3, Stats.getAverage(testData),0);
	}
	
	@Test
	public void testMax() {
		assertEquals(5, Stats.getMaximum(testData));
	}
	
	

}
